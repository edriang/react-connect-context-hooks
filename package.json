{
  "name": "react-connect-context-hooks",
  "version": "0.5.2",
  "description": "Redux-like connect bindings to use along useReducer + useContext.",
  "keywords": [
    "react",
    "connect",
    "useReducer",
    "useContext",
    "state",
    "actions",
    "state management"
  ],
  "author": "edriang",
  "license": "MIT",
  "homepage": "https://edriang.github.io/react-connect-context-hooks",
  "repository": "edriang/react-connect-context-hooks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
